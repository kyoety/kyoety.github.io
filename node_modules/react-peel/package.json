{
  "name": "react-peel",
  "description": "react-peel is a react library to create realistic peeling effects. No dependencies.",
  "version": "0.1.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "dev:docs": "island dev docs",
    "build:docs": "island build docs",
    "preview:docs": "island start docs"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/react-peel.es.js",
      "require": "./dist/react-peel.umd.js"
    }
  },
  "main": "./dist/react-peel.umd.js",
  "module": "./dist/react-peel.es.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/iqbal-rashed/react-peel"
  },
  "keywords": [
    "react-peel",
    "peel.js",
    "peel animation",
    "react"
  ],
  "bugs": {
    "url": "https://github.com/iqbal-rashed/react-peel/issues"
  },
  "homepage": "https://iqbal-rashed.github.io/react-peel",
  "peerDependencies": {
    "react": "16.8.0 || >=17.x || >=18.x",
    "react-dom": "16.8.0 || >=17.x || >=18.x"
  },
  "devDependencies": {
    "@types/node": "^20.8.8",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "islandjs": "^0.7.31",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.1",
    "vite-plugin-linter": "^2.0.7",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
